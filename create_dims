create table DIM_GRUPO (
NUMERO_GRUPO numeric(2) NOT NULL PRIMARY KEY,         -- DIM_GRUPO
NOME_GRUPO varchar(255) NOT NULL
);
create table DIM_PESSOA (
NUMERO_PESSOA numeric(6) NOT NULL PRIMARY KEY,        -- DIM_PESSOA
NOME_PESSOA varchar(255) NOT NULL
);
--create table DIM_ (
--DATA_DESPESA datetime NOT NULL,           -- DIM_TEMPO
--);
create table DIM_EMPENHO (
TIPO_EMPENHO numeric(2) NOT NULL,
DESCRICAO_EMPENHO varchar(50) NOT NULL,
NUMERO_EMPENHO numeric(5) NOT NULL PRIMARY KEY
);
create table DIM_PROCESSO (
CODIGO_PROCESSO_CONTRATACAO char(2) NOT NULL PRIMARY KEY,
DESCRICAO_PROCESSO_CONTRATACAO varchar(50) NOT NULL
);
create table DIM_ORGAO (
NUMERO_ORGAO numeric(4) NOT NULL PRIMARY KEY,
CODIGO_ORGAO numeric(4) NOT NULL,
NOME_ORGAO varchar(255) NOT NULL
);
create table DIM_UNIDADE_ORCAMENTARIA (
NUMERO_UNIDADE_ORCAMENTARIA numeric(4) NOT NULL PRIMARY KEY,
CODIGO_UNIDADE_ORCAMENTARIA numeric(4) NOT NULL,
NOME_UNIDADE_ORCAMENTARIA varchar(255) NOT NULL
);
create table DIM_PROJETO_ATIVIDADE (
NUMERO_PROJETO_ATIVIDADE numeric(4) NOT NULL PRIMARY KEY,
CODIGO_PROJETO_ATIVIDADE numeric(4) NOT NULL,
NOME_PROJETO_ATIVIDADE varchar(255) NOT NULL
);
create table DIM_RUBRICA (
NUMERO_RUBRICA numeric(5) NOT NULL PRIMARY KEY,
CODIGO_RUBRICA numeric(12) NOT NULL,
NOME_RUBRICA varchar(255) NOT NULL
);
create table DIM_VINCULO_ORCAMENTO (
NUMERO_VINCULO_ORCAMENTO numeric(4) NOT NULL PRIMARY KEY,
CODIGO_VINCULO_ORCAMENTO numeric(4) NOT NULL,
NOME_VINCULO_ORCAMENTO varchar(255) NOT NULL
);
