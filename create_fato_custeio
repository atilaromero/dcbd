create table FATO_CUSTEIO (
ID_ZPRT_DESP_CUSTEIO numeric(7) PRIMARY KEY,
NUMERO_GRUPO numeric(2) NOT NULL foreign key references DIM_GRUPO(NUMERO_GRUPO),         -- DIM_GRUPO
-- NOME_GRUPO varchar(255) NOT NULL,
NUMERO_PESSOA numeric(6) NOT NULL foreign key references DIM_PESSOA(NUMERO_PESSOA),        -- DIM_PESSOA
-- NOME_PESSOA varchar(255) NOT NULL,
DATA_DESPESA datetime NOT NULL foreign key references DIM_TEMPO(PK_Data),           -- DIM_TEMPO
-- TIPO_EMPENHO numeric(2) NOT NULL,
-- DESCRICAO_EMPENHO varchar(50) NOT NULL,
NUMERO_EMPENHO numeric(5) NOT NULL foreign key references DIM_EMPENHO(NUMERO_EMPENHO),
CODIGO_PROCESSO_CONTRATACAO char(2) NOT NULL foreign key references DIM_PROCESSO(CODIGO_PROCESSO_CONTRATACAO),
-- DESCRICAO_PROCESSO_CONTRATACAO varchar(50) NOT NULL,
-- NUMERO_PROCESSO_CONTRATACAO numeric(18),
NUMERO_ORGAO numeric(4) NOT NULL foreign key references DIM_ORGAO(NUMERO_ORGAO),
-- CODIGO_ORGAO numeric(4) NOT NULL,
-- NOME_ORGAO varchar(255) NOT NULL,
NUMERO_UNIDADE_ORCAMENTARIA numeric(4) NOT NULL foreign key references DIM_UNIDADE_ORCAMENTARIA(NUMERO_UNIDADE_ORCAMENTARIA),
-- CODIGO_UNIDADE_ORCAMENTARIA numeric(4) NOT NULL,
-- NOME_UNIDADE_ORCAMENTARIA varchar(255) NOT NULL,
NUMERO_PROJETO_ATIVIDADE numeric(4) NOT NULL foreign key references DIM_PROJETO_ATIVIDADE(NUMERO_PROJETO_ATIVIDADE),
-- CODIGO_PROJETO_ATIVIDADE numeric(4) NOT NULL,
-- NOME_PROJETO_ATIVIDADE varchar(255) NOT NULL,
NUMERO_RUBRICA numeric(5) NOT NULL foreign key references DIM_RUBRICA(NUMERO_RUBRICA),
-- CODIGO_RUBRICA numeric(12) NOT NULL,
-- NOME_RUBRICA varchar(255) NOT NULL,
NUMERO_VINCULO_ORCAMENTO numeric(4) NOT NULL foreign key references DIM_VINCULO_ORCAMENTO(NUMERO_VINCULO_ORCAMENTO),
-- CODIGO_VINCULO_ORCAMENTO numeric(4) NOT NULL,
-- NOME_VINCULO_ORCAMENTO varchar(255) NOT NULL,
VALOR_EMPENHADO numeric(14,2) NOT NULL,
VALOR_EMPENHADO_DOLAR numeric(31,17),
VALOR_EMPENHADO_IGP numeric(38,11),
VALOR_EMPENHADO_IPCA numeric(38,11),
VALOR_LIQUIDADO numeric(14,2),
VALOR_LIQUIDADO_DOLAR numeric(31,17),
VALOR_LIQUIDADO_IGP numeric(38,11),
VALOR_LIQUIDADO_IPCA numeric(38,11),
VALOR_PAGO numeric(14,2),
VALOR_PAGO_DOLAR numeric(31,17),
VALOR_PAGO_IGP numeric(38,11),
VALOR_PAGO_IPCA numeric(38,11)
--,CUSTOSA varchar(3) NOT NULL
); 
